{
  "manifest_version": 3,
  "name": "ChatGPT Category Manager",
  "version": "1.0",
  "description": "Organize ChatGPT conversations by categories",
  "permissions": ["storage"],
  "host_permissions": ["https://chat.openai.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ]
}